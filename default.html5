<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$if(css)$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$else$
  <style type="text/css">
body{background-color:#fff;color:#333;font-family:Charter,"Bitstream Charter",Palatino,Georgia,serif;line-height:1.4;margin:2em auto;max-width:35em;text-align:left;width:93%;word-wrap:break-word}header{text-align:center}h1,h2,h3,h4,h5,h6{font-family:Lato,Helvetica,sans-serif}blockquote{border-left:4px solid #ccc;color:#444;margin:0;padding:0 0 0 16px}code{background-color:#eee;padding-left:4px;padding-right:4px}pre code{display:block;line-height:1.2em;max-height:30em;overflow:auto;padding:10px}img{max-width:100%;height:auto}table{border-bottom:2px solid #333;border-collapse:collapse;border-top:2px solid #333;margin:0 auto}table th{border-bottom:1px solid #333;font-weight:normal;padding:0 10px;vertical-align:top}table td{padding:0 10px;vertical-align:top}tbody tr:nth-child(even){background-color:#f9f9f9}tbody tr:nth-child(odd){background-color:#fff}
  </style>
$endif$
$if(math)$
  <script type="text/x-mathjax-config">
    MathJax.Hub.Config({
      "HTML-CSS": {
        // TeX font has better spacing on Linux browsers
        availableFonts: ["TeX"],
        preferredFont: "TeX"
      }
    });
  </script>
  <script type="text/javascript"
    src="https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML">
  </script>
$endif$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<p class="subtitle"><em>$subtitle$</em></p>
$endif$
$if(author)$
<p class="author">
<em>by $for(author)$$author$$sep$, $endfor$</em>
</p>
$endif$
$if(date)$
<p class="date"><em>Substantive revision $date$</em></p>
$endif$
</header>
$endif$
$if(toc)$
<h2>Contents</h2>
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
